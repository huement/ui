@include superclass() {
  // Navbar
  .navbar {
    align-items: stretch;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    background-color: getPageColor("navbar");
    z-index: $zindex-navbar;
    @if ($border-radius > 0) {
      border-radius: $border-radius;

      &.navbar-sm {
        border-radius: $border-radius-sm;
      }

      &.navbar-lg {
        border-radius: $border-radius-lg;
      }
    }

    &.navbar-alt {
      background-color: getPageColor("navbar-alt");
    }

    &.navbar-dark {
      background-color: getPageColor("navbar-dark");
    }

    .navbar-section {
      align-items: center;
      display: flex;
      flex: 1 0 0;

      &:not(:first-child):last-child {
        justify-content: flex-end;
      }
    }

    .navbar-center {
      align-items: center;
      display: flex;
      flex: 0 0 auto;
      justify-content: center;

      img,
      svg,
      .logo {
        min-width: $sz_9;
        max-height: 100%;
        max-width: 100%;
        margin: 0 auto;
        position: relative;
        display: block;
      }
    }

    .navbar-brand {
      font-size: getFontSize("lg");
      text-decoration: none;
    }

    margin-top: vr-block(0.5);
    margin-bottom: vr-block(1);
  }
}

{
  "name": "mojo",
  "private": false,
  "version": "0.6.0",
  "type": "module",
  "main": "src/main.ts",
  "license": "MPL-2.0",
  "author": "Derek Scott <derek@huement.com>",
  "homepage": "https://github.com/huement/mojo",
  "buildNumber": 289,
  "bundlesize": [
    {
      "path": "./dist/mojo.min.css",
      "maxSize": "14.5 kB"
    }
  ],
  "scripts": {
    "bump": "node ./bump.cjs",
    "clean": "rimraf dist",
    "copy": "node ./web/copy.cjs",
    "build": "yarn css && yarn demo",
    "watch": "nodemon -e scss -x \"yarn build\" & browser-sync --config bs-config.cjs",
    "watch:css": "nodemon -e scss -x \"yarn css\" & browser-sync --config bs-config.cjs",
    "css": "sass --verbose --no-quiet --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css src:dist/css && yarn bump",
    "publish": "yarn clean && yarn lint && yarn css && yarn prefix && yarn minify",
    "lint": "stylelint src/**/*.{css,scss} --cache --cache-location .cache/.stylelintcache",
    "lintjs": "eslint src/**/*.{ts,js}",
    "prefix": "postcss --config postcss.config.cjs --replace dist/css/mojo.css",
    "minify": "cleancss -O01 --source-map --source-map-inline-sources --output dist/mojo.min.css --format 'breaks:afterBlockBegins=on;spaces:aroundSelectorRelation=on' dist/css/mojo.css",
    "pug": "node web/build.cjs",
    "demo": "yarn demo:pug && yarn demo:css",
    "demo:pug": "yarn pug && yarn copy",
    "demo:css": "sass --quiet --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css web/assets/style/website.scss dist/demo/css/website.css",
    "sass:render": "npx build-scss src dev -s -d -x",
    "sass:publish": "npx build-scss src dev -p"
  },
  "files": [
    "dist/{css,js}/*.{css,js,map}",
    "dist/*.{css,js,map}",
    "src"
  ],
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "src"
    ],
    "ext": "scss",
    "ignore": [
      "*.css"
    ],
    "delay": "1000"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.0.0",
    "ansi-colors": "^4.1.3",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "jstransformer": "^1.0.0",
    "jstransformer-highlight": "^2.0.0",
    "jstransformer-markdown-it": "^3.0.0",
    "jstransformer-prismjs": "^0.1.0",
    "jstransformer-scss": "^2.0.0",
    "node-html-parser": "^6.1.12",
    "nodemon": "^3.0.2",
    "postcss": "^8.0.0",
    "pug": "^3.0.2",
    "rollup": "^4.9.1",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "scss-json": "^1.0.0",
    "stylelint": "^16.0.2",
    "stylelint-scss": "^6.0.0",
    "terminal-img": "^2.0.24",
    "typescript": "*",
    "vite": "^5.0.8",
    "vite-plugin-browser-sync": "^2.0.1",
    "vite-plugin-clean": "^1.0.0",
    "vite-plugin-print-info": "^0.0.3",
    "vite-plugin-print-urls": "^0.1.2",
    "vite-plugin-pug": "^0.3.2",
    "vite-plugin-run": "^0.5.1"
  },
  "dependencies": {
    "@squirrel-forge/build-scss": "^0.8.4",
    "@squirrel-forge/sass-base64-loader": "^0.11.2",
    "@squirrel-forge/sass-package-importer": "^0.11.2",
    "autoprefixer": "^10.4.16",
    "bootstrap": "5.3.2",
    "browser-sync": "^3.0.2",
    "bundlesize": "^0.18.1",
    "clean-css-cli": "^5.6.3",
    "copyfiles": "^2.4.1",
    "jsonfile-updater": "^3.1.0",
    "normalize-scss": "^8.0.0",
    "nouislider": "^15.7.1",
    "postcss-cli": "^11.0.0",
    "recursive-copy": "^2.0.14",
    "rimraf": "^5.0.5",
    "sass-unused": "^0.5.0",
    "stylelint-config-twbs-bootstrap": "^13.0.0",
    "trash": "^8.1.1"
  },
  "engines": {
    "node": ">=18.19.0"
  }
}
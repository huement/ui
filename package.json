  "scripts": {
    "clean": "rimraf dist",
    "bump": "node ./bin/bump.cjs",
    "copyIDK": "node ./bin/copy.cjs",
    "pug": "node bin/pug.cjs",
    "watch": "nodemon -x \"npm run build\" & browser-sync start --config web/bs-config.cjs",
    "scss": "sass --verbose --no-quiet --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css scss:dist/css && npm run bump",
    "postfix": "postcss --config postcss.config.cjs --replace dist/mojo.css",
    "minify": "cleancss -O01 --output dist/mojo.min.css --format 'breaks:afterBlockBegins=on;spaces:aroundSelectorRelation=on' dist/css/mojo.css dist/hi/icons-s3.min.css",
    "build": "npx npm-run-all -lsc pug scss postfix minify"
  },
  "files": [
    "dist/hi/*.{css,json,ttf,woff,woff2,map}",
    "dist/fonts/**/*.{ttf,woff,woff2,css,html}",
    "dist/{css,js}/*.{css,js,map}",
    "dist/*.{css,js,map}",
    "scss"
  ],
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "scss",
      "public"
    ],
    "ext": "scss,html,ts,json,cjs",
    "ignore": [
      ".cache/**"
    ],
    "delay": 1500
  },
  "publish": [
    "dist/mojo.css",
    "dist/mojo.min.css",
    "dist/mojo.css.map",
    "dist/mojo.min.css.map",
    "dist/js",
    "dist/hi",
    "dist/jollybar",
    "dist/fonts/icons",
    "dist/fonts/Jost",
    "dist/fonts/Saira",
    "dist/fonts/Space_Grotesk"
  ],

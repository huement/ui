{
  "name": "@huement/ui",
  "codeName": "mojo",
  "private": false,
  "version": "0.5.1",
  "type": "module",
  "sourceType": "module",
  "license": "MPL-2.0",
  "author": "Derek Scott <derek@huement.com>",
  "homepage": "https://github.com/huement/ui",
  "buildNumber": 537,
  "buildDate": "Sun Oct 13 2024",
  "buildSize": "383.01 KiB",
  "scripts": {
    "clean": "rimraf dist",
    "bump": "node ./bin/bump.cjs",
    "newVersion": "npm version patch && npx npm-run-all -l -s -c  build:icons build:js build deploy",
    "dev": "nodemon -x \"npm run build\"",
    "watch:css": "nodemon -x \"npm run scss\"",
    "scss": "sass --verbose --no-quiet --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css scss:dist && npm run bump",
    "build": "npx npm-run-all -lsc scss minify minify:S3 postfix postfix:S3",
    "stats": "npx wallace dist/mojo.css",
    "build:css": "npx npm-run-all -lsc scss minify minify:S3 postfix postfix:S3 stats",
    "build:js": "node bin/bundle.cjs",
    "build:icons": "npm run scss:icons && node bin/icons.cjs && npm run minify:icon && npm run minify:iconS3",
    "deploy": "node bin/s3sync.cjs",
    "lint": "stylelint --fix scss/**/*.{css,scss} --formatter verbose --config .stylelintrc.json --cache --cache-location .cache/.stylelintcache",
    "lintjs": "eslint --fix --config .eslintrc.cjs js/*.js js/**/*.js",
    "postfix": "postcss --config postcss.config.cjs --replace dist/mojo.min.css",
    "postfix:S3": "postcss --config postcss.config.cjs --replace dist/mojo-s3.min.css",
    "minify": "cleancss -O01 --output dist/mojo.min.css --format 'breaks:afterBlockBegins=on;spaces:aroundSelectorRelation=on' dist/mojo.css dist/hi/icons.css",
    "minify:S3": "cleancss -O01 --output dist/mojo-s3.min.css --format 'breaks:afterBlockBegins=on;spaces:aroundSelectorRelation=on' dist/mojo.css dist/hi/icons-s3.css",
    "minify:icon": "cleancss -O01 --output dist/hi/icons.min.css --format 'breaks:afterBlockBegins=on;spaces:aroundSelectorRelation=on' dist/hi/icons.css dist/hi/sizing.css",
    "minify:iconS3": "cleancss -O01 --output dist/hi/icons-s3.min.css --format 'breaks:afterBlockBegins=on;spaces:aroundSelectorRelation=on' dist/hi/icons-s3.css dist/hi/sizing.css",
    "clean:icons": "npx svgo -f ./tokens/icons/raw -o ./tokens/icons/clean --config svgo.config.js",
    "scss:all": "npx npm-run-all -lsc scss:demo scss:jolly scss:icons scss",
    "scss:jolly": "sass --quiet --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css jollybar/debugger.scss dist/jollybar/jollybar.css",
    "scss:icons": "sass --quiet --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css tokens/icons/sizing.scss dist/hi/sizing.css"
  },
  "files": [
    "dist/fonts/**/*.{ttf,woff,woff2,css,html}",
    "dist/{css,js}/*.{css,js,map}",
    "dist/*.{css,js,map}",
    "scss",
    "web/**/*.pug"
  ],
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "scss",
      "public",
      "web"
    ],
    "ext": "scss,html,ts,json,cjs,pug",
    "ignore": [
      ".cache/**"
    ],
    "delay": 1500
  },
  "publish": [
    "dist/mojo.css",
    "dist/mojo.min.css",
    "dist/mojo.css.map",
    "dist/mojo.min.css.map",
    "dist/js",
    "dist/jollybar",
    "dist/fonts/icons",
    "dist/fonts/Jost",
    "dist/fonts/Saira",
    "dist/fonts/Space_Grotesk"
  ],

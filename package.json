{
  "name": "@huement/ui",
  "codeName": "mojo",
  "private": false,
  "version": "0.8.8",
  "type": "module",
  "sourceType": "module",
  "license": "MPL-2.0",
  "author": "Derek Scott <derek@huement.com>",
  "homepage": "https://github.com/huement/ui",
  "buildNumber": 630,
  "buildDate": "Sun Jun 29 2025",
  "buildSize": "369.13 KiB",
  "hui": {
    "mqResponsive": "true",
    "baseFontSize": "16px",
    "baseLineHeight": 1.5,
    "responsive": "true",
    "tokens": "tokens/chords.json"
  },
  "scripts": {
    "clean": "rimraf dist",
    "bump": "node ./bin/bump.cjs && npm run stats",
    "newVersion": "npm version patch && npx npm-run-all -l -s -c build:js build deploy stats",
    "dev": "nodemon -x \"npm run build\"",
    "scss": "sass --verbose --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css scss:dist && npm run bump",
    "build": "npx npm-run-all -lsc scss minify postfix",
    "stats": "npx wallace dist/ui.css",
    "stats:min": "npx wallace dist/ui.min.css",
    "build:js": "node bin/bundle.cjs",
    "build:colors": "node bin/cli.cjs color",
    "build:chords": "node bin/cli.cjs chord",
    "deployDO": "node bin/s3sync.cjs",
    "deploy": "npm login && npm publish --access public",
    "lint": "stylelint --fix scss/**/*.{css,scss} --formatter verbose --config .stylelintrc.json --cache --cache-location .cache/.stylelintcache",
    "lint:js": "eslint --fix --config .eslintrc.cjs js/*.js js/**/*.js",
    "postfix": "postcss --config postcss.config.cjs --replace dist/ui.min.css",
    "minify": "cleancss -O01 --output dist/ui.min.css --format 'breaks:afterBlockBegins=on;spaces:aroundSelectorRelation=on' dist/ui.css",
    "scss:jolly": "sass --quiet --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css jollybar/debugger.scss dist/jollybar/jollybar.css"
  },
  "files": [
    "dist/js/*.js",
    "dist/*.{css,map,json}",
    "scss"
  ],
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "scss",
      "public",
      "web"
    ],
    "ext": "scss,html,ts,json,cjs,pug",
    "ignore": [
      ".cache/**"
    ],
    "delay": 1500
  },
  "publish": [
    "dist/ui.css",
    "dist/ui.min.css",
    "dist/ui.css.map",
    "dist/ui.min.css.map",
    "dist/js",
    "dist/generated_colors.json",
    "dist/generated_chords.json"
  ],

{
  "superclass": "plume",
  "mqResponsive": true,
  "baseFontSize": "16px",
  "baseLineHeight": 1.5,
  "binDir": "cli/tasks/run",
  "harmonyJSONFile": "tokens/_tokens-chords-test.json",
  "harmonySCSSFile": "scss/config/_tokens-test.scss",
  "paletteSCSSFile": "build/scss/core/_palette.scss",
  "cache": ".cache",
  "chordTokens": "../tokens/chords.json",
  "colorTokens": "tokens/color_tokens.json",
  "globalTasks": "cli/tasks/run/cmds.json",
  "helpItems": "cli/tasks/lib/help.json",
  "staticMaps": "cli/tasks/lib/maps.json",
  "taskDir": "cli/tasks",
  "dev": {
    "assets": "web/assets",
    "debug": "jollybar",
    "docs": "web/pages/docs",
    "fonts": "tokens/fonts/Webfonts",
    "media": "tokens",
    "style": "build/scss",
    "templates": "web/pages/templates",
    "url": "localhost/ui/",
    "website": "web"
  },
  "prod": {
    "assets": "dist",
    "debug": "docs/jollybar",
    "docs": "docs/docs",
    "fonts": "docs/www/fonts",
    "img": "docs/www/imgs",
    "js": "docs/www/js",
    "pages": "docs/www",
    "style": "docs/css",
    "url": "huement.github.io/ui/"
  },
  "env": {
    "pug": true,
    "handlebars": false,
    "markdown": false,
    "htmlvars": false,
    "outputStyle": "expanded"
  }
}
